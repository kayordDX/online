{
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": { "path": "Logs/log.txt", "rollingInterval": "Day" }
      }
    ],
    "Enrich": ["FromLogContext", "WithMachineName", "WithThreadId"]
  },
  "ConnectionStrings": {
    "DefaultConnection": "host=localhost;database=online;username=online;password=online",
    "Redis": "localhost,password=online,ssl=False,abortConnect=False"
  },
  "JWTOptions": {
    "Secret": "mySuper$ecretKey123!@#ForJWtTokenGeneration",
    "Issuer": "https://oneline.com",
    "Audience": "https://oneline.com",
    "ExpirationTimeInMinutes": 15
  }
}
