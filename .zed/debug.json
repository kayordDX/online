[
  {
    "label": "Debug .NET Core App",
    "adapter": "netcoredbg",
    "request": "launch",
    "program": "${ZED_WORKTREE_ROOT}/api/src/online/bin/Debug/net10.0/online.dll",
    "cwd": "${ZED_WORKTREE_ROOT}/api/src/online",
    "env": {
      "ASPNETCORE_ENVIRONMENT": "Development",
      "ASPNETCORE_URLS": "http://localhost:5000",
    },
    "build": {
      "command": "dotnet",
      "args": [
        "build",
        "${ZED_WORKTREE_ROOT}/api/src/online/online.csproj",
        "-p:Configuration=Debug",
      ],
    },
  },
]
