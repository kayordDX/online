{
	"name": "client",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test",
		"api": "pnpm run api:swagger && npm run api:orval",
		"api:swagger": "curl http://localhost:5017/openapi/v1.json > swagger.json",
		"api:orval": "orval"
	},
	"devDependencies": {
		"@eslint/compat": "^1.4.0",
		"@eslint/js": "^9.37.0",
		"@kayord/ui": "^2.0.3",
		"@lucide/svelte": "^0.546.0",
		"@playwright/test": "^1.56.1",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/kit": "^2.47.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/vite": "^4.1.14",
		"@tanstack/svelte-query": "^6.0.3",
		"@tanstack/table-core": "^8.21.3",
		"@types/qs": "^6.14.0",
		"@vitest/browser": "^3.2.3",
		"eslint": "^9.37.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.12.4",
		"globals": "^16.0.0",
		"jwt-decode": "^4.0.0",
		"layerchart": "2.0.0-next.33",
		"mode-watcher": "^1.1.0",
		"orval": "^7.13.2",
		"playwright": "^1.56.1",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.7.0",
		"qs": "^6.14.0",
		"runed": "^0.35.1",
		"svelte": "^5.40.2",
		"svelte-check": "^4.3.3",
		"svelte-sonner": "^1.0.5",
		"sveltekit-superforms": "^2.27.4",
		"tailwindcss": "^4.1.14",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.46.1",
		"vite": "^7.1.10",
		"vitest": "^3.2.3",
		"vitest-browser-svelte": "^1.1.0",
		"zod": "^4.1.12"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"@tailwindcss/oxide",
			"esbuild"
		]
	}
}
