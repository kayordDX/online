{
	"name": "client",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test",
		"test:install": "pnpm exec playwright install chromium",
		"api": "pnpm run api:swagger && npm run api:orval",
		"api:swagger": "curl http://localhost:5000/openapi/v1.json > swagger.json",
		"api:orval": "orval",
		"hey": "openapi-ts"
	},
	"devDependencies": {
		"@eslint/compat": "^2.0.0",
		"@eslint/js": "^9.39.1",
		"@kayord/ui": "^2.0.11",
		"@lucide/svelte": "^0.560.0",
		"@playwright/test": "^1.57.0",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/kit": "^2.49.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/vite": "^4.1.18",
		"@tanstack/svelte-query": "^6.0.10",
		"@tanstack/table-core": "^8.21.3",
		"@types/qs": "^6.14.0",
		"@vitest/browser": "^4.0.15",
		"@vitest/browser-playwright": "^4.0.15",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.13.1",
		"globals": "^16.5.0",
		"jwt-decode": "^4.0.0",
		"layerchart": "2.0.0-next.33",
		"mode-watcher": "^1.1.0",
		"orval": "^7.17.0",
		"playwright": "^1.57.0",
		"prettier": "^3.7.4",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"qs": "^6.14.0",
		"runed": "^0.37.0",
		"svelte": "^5.45.10",
		"svelte-check": "^4.3.4",
		"svelte-sonner": "^1.0.7",
		"sveltekit-superforms": "^2.28.1",
		"tailwindcss": "^4.1.18",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.9.3",
		"vite": "^7.2.7",
		"vitest": "^4.0.15",
		"vitest-browser-svelte": "^2.0.1",
		"zod": "^4.1.13"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"@tailwindcss/oxide",
			"esbuild"
		]
	}
}
